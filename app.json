{
  "expo": {
    "name": "Eudaimonia",
    "slug": "eudaimonia",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "ios": {
      "bundleIdentifier": "com.zarcainc.eudaimonia",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio"
        ]
      },
      "appStoreUrl": "https://itunes.apple.com/us/app/kitten-tricks/id1246143230"
    },
    "android": {
      "package": "com.zarcainc.eudaimonia",
      "versionCode": 1,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.akveo.kittenTricks"
    },
    "androidStatusBar": {
      "backgroundColor": "#FFFFFF"
    },
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "description": ""
  }
}
